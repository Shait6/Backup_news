{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "westeurope"
    },
    "vaultName": {
      "value": "rsv-backup-${uniqueString(resourceGroup().id)}"
    },
    "backupPolicyName": {
      "value": "DefaultPolicy"
    },
    "backupRetentionDays": {
      "value": 30
    },
    "backupFrequency": {
      "value": "Daily"
    },
    "weeklyBackupDaysOfWeek": {
      "value": [
        "Sunday",
        "Wednesday"
      ]
    }
  }
}